    <recipe>
        <title>Blackeyed Peas</title>
        <ingreidentslist>
            <ingredient>
                <quantity>1</quantity>
                <units>C.</units>
                <fooditem>unbleached wheat blend flour</fooditem>
            </ingredient>
            <ingredient>
                <quantity>2</quantity>
                <units>tsp.</units>
                <fooditem>baking powder</fooditem>
            </ingredient>
            <ingredient>
                <quantity>1</quantity>
                <units>tsp.</units>
                <fooditem>unrefined sugar</fooditem>
            </ingredient>
            <ingredient>
                <quantity>1</quantity>
                <units>tsp.</units>
                <fooditem>coarse kosher salt</fooditem>
            </ingredient>
            <ingredient>
                <quantity>1</quantity>
                <units></units>
                <fooditem>free-range egg</fooditem>
            </ingredient>
            <ingredient>
                <quantity>1</quantity>
                <units>C.</units>
                <fooditem>hormone-free milk</fooditem>
            </ingredient>
            <ingredient>
                <quantity>1</quantity>
                <units>tsp.</units>
                <fooditem>organic vegetable oil</fooditem>
            </ingredient>
        </ingreidentslist>
        <preparation>
            <step>
                <equipment>mixing bowl</equipment>
                <equipment>griddle</equipment>
            </step>
        </preparation>
    </recipe>
